<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.dsproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpScreen" />
        <activity android:name=".ReservationActivity" />
        <activity android:name=".MyProfile" />
        <activity android:name=".EditProfile" />
        <activity android:name=".AddBalance" />
        <activity android:name=".BookTicket" />
        <activity android:name=".CancelTicket" />
        <activity android:name=".BookedHistory"></activity>
    </application>

</manifest>